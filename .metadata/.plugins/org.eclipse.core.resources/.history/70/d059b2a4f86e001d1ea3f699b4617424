    <script>
    $(document).ready(function(){
    	var id = $("#id").val();
    	var pw = $("#pw").val();
    	
    	if(id=="" || pw==""){
    		alert("아이디 또는 비밀번호 입력칸이 비어있습니다");
    		return false;
    	}
    
    })
    
